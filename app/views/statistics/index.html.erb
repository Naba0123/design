<h1>統計分析</h1>
<div class="tabbable">
<ul class="nav nav-tabs">
<% @queryNumber.times do |n| %>
  <li><a href="#<%= n+1 %>" data-toggle="tab">条件 <%= n+1 %></a></li>
<% end %>

</ul>

<form>
<div class="tab-content">
<% @queryNumber.times do |n| %>
<div class="tab-pane" id="<%= n+1 %>">

<%= search_form_for @search[n],:url=>{:controller=>params[:controller]} , :html => { class: "form-horizontal" } do |f| %>
  <div class="form-group">
    <%= f.label :user_id_cont, class: "col-sm-2 control-label"  %>
    <div class="col-sm-10">
      <%= f.text_field :user_id_cont, class: "form-control" , name:"q[][user_id_cont]" %>
    </div>
  </div>

<% end %>
</div>

<% end %>
</div>
<input type="submit">
</form>
<p>
検索内容を含む生徒数
<ul>
<% @search.each do |r| %>
<li><%= r.result.size %>人</li>
<% end %>
</ul>
</p>
<%= @queryString %>