<h1>Listing events</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Meeting date</th>
      <th>Meeting place</th>
      <th>Event type</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.meeting_date %></td>
        <td><%= event.meeting_place %></td>
        <td><%= event.event_type %></td>
        <td><%= link_to 'Show', event %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>


