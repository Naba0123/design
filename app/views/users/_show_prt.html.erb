<h4>参加者情報</h4>
<dl class="dl-horizontal">
  <dt>ID</dt>
  <dd><%= @user.id %></dd>
  <dt>希望課程</dt>
  <dd><%= Department.find(@user.participant.wish).name %></dd>
  <dt>志望コース</dt>
  <dd><%= ResearchRoom.find(@user.participant.wish_course).name %></dd>
  <dt>志望指導教員名</dt>
  <dd><%= @user.participant.teacher %></dd>
  <% if @user.participant.is_entered %>
  <dt>入学済み</dt>
  <dd></dd>
  <dt>入学日</dt>
  <dd><%= @user.participant.entry_date %></dd>
  <% end %>
  <dt>過去の参加履歴</dt>
  <dd><% @members.each do |guidance| %>
   <p><td><%= guidance.id  %></td>
   <td><%= guidance.guidance_date %></td>
   <td><%= guidance.guidance_place %></td>
   <td><%= guidance.guidance_kind.try(:name) %></td>
   </p>
   <% end %>
   <dd>
</dl>
